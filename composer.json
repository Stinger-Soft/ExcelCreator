{
	"name" : "stinger-soft/excel-creator",
	"type" : "library",
	"description" : "Creates Excel export by defining data bindings",
	"keywords" : [
		"Excel",
		"PHP",
		"Data binding"
	],
	"homepage" : "http://www.stinger-soft.net",
	"authors" : [{
			"name" : "Florian Meyer",
			"email" : "florian.meyer@stinger-soft.net",
			"role" : "Developer"
		}
	],
	"autoload" : {
		"psr-4" : {
			"StingerSoft\\ExcelCreator\\" : "src/StingerSoft/ExcelCreator"
		},
		"exclude-from-classmap" : [
			"/tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"StingerSoft\\ExcelCreator\\" : "tests/StingerSoft/ExcelCreator"
		}
	},
	"license" : "Apache-2.0",
	"require" : {
		"php": "^7.0",
		"doctrine/collections" : "~1.2",
		"phpoffice/phpspreadsheet" : "^1.2.0",
		"stinger-soft/php-commons" : "~1.0",
		"symfony/property-access" : "^3.0|^4.0",
		"symfony/translation" : "^3.0|^4.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~7.3.5",
		"php-coveralls/php-coveralls" : "~2.1.0"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.0.x-dev",
			"dev-develop" : "1.1.x-dev"
		}
	}
}