{
	"name" : "stinger-soft/excel-creator",
	"type" : "library",
	"description" : "Creates Excel export by defining data bindings",
	"keywords" : [
		"Excel",
		"PHP",
		"Data binding"
	],
	"homepage" : "http://www.stinger-soft.net",
	"authors" : [{
			"name" : "Florian Meyer",
			"email" : "florian.meyer@stinger-soft.net",
			"role" : "Developer"
		}
	],
	"autoload" : {
		"psr-4" : {
			"StingerSoft\\ExcelCreator\\" : "src/StingerSoft/ExcelCreator"
		},
		"exclude-from-classmap" : [
			"/tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"StingerSoft\\ExcelCreator\\" : "tests/StingerSoft/ExcelCreator"
		}
	},
	"license" : "Apache-2.0",
	"require" : {
		"php": "^8.1",
		"openspout/openspout": "^4.7",
		"doctrine/collections" : "^2",
		"phpoffice/phpspreadsheet" : "^2",
		"stinger-soft/php-commons" : "^1",
		"symfony/property-access" : "^6",
		"symfony/contracts" : "^3.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^8||^9||^10",
		"php-coveralls/php-coveralls" : "^2"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "3.0.x-dev",
			"dev-develop" : "4.0.x-dev"
		}
	}
}
